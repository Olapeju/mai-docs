{
  "name": "mai-docs",
  "version": "1.0.0",
  "description": "simple doc-man",
  "main": "index.js",
  "scripts": {
    "test:frontend": "NODE_ENV=test mocha -w test/mocha-helper.js test/**/*.spec.js --slow 5000 --compilers js:babel-register",
    "start:webpack": "webpack",
    "test:cover": "NODE_ENV=test npm run test:cover:server && npm run test:cover:src",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text node_modules/.bin/_mocha --reporter spec ./tools/testSetup.js ./server/test/*.test.js ./src/test/**/*.test.js --require babel-register --timeout 20000",
    "test:front": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/_mocha --reporter spec ./tools/testSetup.js ./src/test/**/*.test.js --require babel-register --timeout 20000",
    "test:client": "NODE_ENV=test nyc --reporter=text  --reporter=html --extension .jsx ./node_modules/mocha/bin/_mocha client/tests/testSetup.js client/tests/components/**/*spec.js client/tests/**/*spec.js",
    "test:watch": "npm run test -- --watch",
    "coveralls": "npm test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test:e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js --require babel-register",
    "db:seed": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "db:test:seed": "NODE_ENV=development babel-node ./server/test/helper/seeder.js",
    "db:clear:seed": "NODE_ENV=test babel-node ./server/test/helper/clearDb.js",
    "start": "npm run build && nodemon ./index.js --exec babel-node",
    "build": "babel-node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-milesanmi/mai-docs.git"
  },
  "keywords": [],
  "author": "Maranatha A. Ilesanmi",
  "bugs": {
    "url": "https://github.com/andela-milesanmi/mai-docs/issues"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "**/*.test.jsx",
      "dist"
    ],
    "extension": [
      ".jsx",
      ".js",
      ".es6"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "homepage": "https://github.com/andela-milesanmi/mai-docs#readme",
  "license": "MIT",
