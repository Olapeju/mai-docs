{
  "name": "mai-docs",
  "version": "1.0.0",
  "description": "[![Coverage Status](https://coveralls.io/repos/github/andela-milesanmi/mai-docs/badge.svg?branch=develop)](https://coveralls.io/github/andela-milesanmi/mai-docs?branch=master) [![Code Climate](https://codeclimate.com/github/andela-milesanmi/mai-docs/badges/gpa.svg)](https://codeclimate.com/github/andela-milesanmi/mai-docs/) [![Build Status](https://travis-ci.org/andela-milesanmi/mai-docs.svg?branch=master)](https://travis-ci.org/andela-milesanmi/mai-docs) [![license](https://img.shields.io/github/license/mashape/apistatus.svg)]()",
  "main": "tools/srcServer.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:client lint:watch",
    "open:client": "nodemon ./tools/srcServer --exec babel-node",
    "build": "babel client -d dist",
    "serve": "node dist/bundle.js",
    "lint": "node_modules/.bin/esw client tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "npm run db:test-seed && npm run test:mocha",
    "test:mocha": "cross-env NODE_ENV=test mocha ./server/**/*.test.js --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "db:test-seed": "cross-env NODE_ENV=test babel-node ./server/tests/helper/seed-helper.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-milesanmi/mai-docs.git"
  },
  "author": "Maranatha A. Ilesanmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-milesanmi/mai-docs/issues"
  },
  "homepage": "https://github.com/andela-milesanmi/mai-docs#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "jquery": "^3.2.1",
    "jsdom": "^10.1.0",
    "jsonwebtoken": "^7.4.1",
    "morgan": "^1.8.1",
    "pg": "^6.2.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.4",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "jsdom": "^10.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.4.1",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
