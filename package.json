{
  "name": "mai-docs",
  "version": "1.0.0",
  "description": "Mai Document Management System",
  "scripts": {
    "start": "NODE_ENV=production npm run build",
    "open:client": "babel-node ./tools/appServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* client tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test nyc --reporter=text mocha ./client/test/**/*.test.js ./server/test/**/*.test.js --require babel-register",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node ./tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-milesanmi/mai-docs.git"
  },
  "author": "Maranatha A. Ilesanmi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-milesanmi/mai-docs/issues"
  },
  "homepage": "https://github.com/andela-milesanmi/mai-docs#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.1",
    "colors": "^1.1.2",
    "css-loader": "0.23.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.5",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^4.1.0",
    "file-loader": "0.8.5",
    "jquery": "2.2.3",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "key-mirror": "^1.0.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.98.2",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^1.8.0",
    "pg": "^6.2.3",
    "pg-hstore": "^2.3.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-paginate": "^4.4.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-select": "^1.0.0-rc.5",
    "react-tinymce": "^0.5.1",
    "redux": "3.5.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-thunk": "2.0.1",
    "sass-loader": "^3.1.2",
    "script-loader": "^0.6.1",
    "sequelize": "^3.30.4",
    "style-loader": "0.13.1",
    "sweetalert": "^1.1.3",
    "toastr": "2.1.2",
    "url-loader": "0.5.7",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "coveralls": "^2.13.1",
    "cross-env": "1.0.7",
    "enzyme": "2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "8.5.0",
    "jsdom-global": "3.0.2",
    "json-loader": "^0.5.4",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "nyc": "^11.0.2",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
    "sinon": "^2.3.4",
    "supertest": "^3.0.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "2.10.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "client/test",
      "server/test"
    ]
  }
}
