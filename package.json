    "should": "^11.2.1",
    "style-loader": "^0.17.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
